#include <stdio.h>#include <stdlib.h>int main(){ int i, j,n,k,s; int result = 0; int arr[101][11]={0}; for(k=1;k<=9;k++) arr[1][k]=1; scanf("%d",&n);  for(i=2;i<=n;i++){  for(j=0;j<=9;j++){   if(j==0){    arr[i][0]=arr[i-1][1];    arr[i][0]%=1000000000;   }   else if(j==9){    arr[i][9]=arr[i-1][8];    arr[i][9]%=1000000000;   }   else {   arr[i][j]=arr[i-1][j-1]+arr[i-1][j+1];   arr[i][j]%=1000000000;  }  } } for(s=0;s<=9;s++){ result+=arr[n][s]; result%= 1000000000; } printf("%d",result);    }
