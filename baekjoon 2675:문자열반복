#include<stdio.h>
#include<string.h>
int main(void)
{
int i,j,k,count,count2,length;
scanf("%d",&count);
char str[30]=" ";
for(i=0;i<count;i++){
	scanf("%d %s",&count2,str);
	length=strlen(str);
	for(j=0;j<length;j++){
		for(k=0;k<count2;k++)
		printf("%c",str[j]);
	}
	printf("\n");
	
}
return 0;

}
